5. Check Happy Number
def sum_square(n):
    return sum(int(d) ** 2 for d in str(n))

n = int(input())
slow = fast = n

while True:
    slow = sum_square(slow)
    fast = sum_square(sum_square(fast))
    if slow == fast:
        break

print("Happy Number" if slow == 1 else "Not Happy")
